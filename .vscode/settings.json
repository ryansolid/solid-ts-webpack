{
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": "explicit",
        "source.fixAll.stylelint": "explicit"
    },
    "eslint.enable": true,
    "stylelint.enable": true,
    "stylelint.configFile": ".stylelintrc.cjs",
    "stylelint.validate": ["scss"],
    "scss.validate": true
}
